      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String" xml:space="preserve">Select M.NIM, M.Nama, M.Angkatan, P.Nama_Program_Studi, Jumlah_SKS, SUM(MK.SKS *  Nilai_Angka.Nilai_Angka) Total_Nilai, SUM(MK.SKS *  Nilai_Angka.Nilai_Angka)/Jumlah_SKS IP_Sementara From nilai N INNER Join mata_kuliah MK On MK.Id_Mata_Kuliah = N.Id_Mata_Kuliah INNER Join mahasiswa M On M.NIM = N.NIM INNER Join program_studi P On M.Id_Program_Studi = P.Id_Program_Studi INNER Join (Select SUM(MK2.sks) Jumlah_SKS, N2.NIM From mata_kuliah MK2 INNER Join nilai N2 On MK2.Id_Mata_Kuliah = N2.Id_Mata_Kuliah Group By N2.NIM) Jumlah_SKS On Jumlah_SKS.NIM = N.NIM INNER Join (Select Case When N3.Nilai = 'A' Then 4 When N3.Nilai = 'AB' Then 3.5 When N3.Nilai = 'B' Then 3 When N3.Nilai = 'BC' Then 2.5 When N3.Nilai = 'C' Then 2 When N3.Nilai = 'D' Then 1 When N3.Nilai = 'E' Then 0 End Nilai_Angka, N3.Id_Nilai From Nilai N3) Nilai_Angka On Nilai_Angka.Id_Nilai = N.Id_Nilai </InArgument>
      </Assign.Value>
    </Assign>
